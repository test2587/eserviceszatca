      if ($activNo < 4) {
        for (i = 0; i < 7; i++) {
          pages.eq(i).show();
        }
      } else if ($activNo >= pagesNo - 6) {
        for (i = pagesNo; i > pagesNo - 12; i--) {
          pages.eq(i).show();
        }
      } else {
        for (i = $activNo - 3; i <= $activNo + 3; i++) {
          pages.eq(i).show();
        }
      }
    }
  }
