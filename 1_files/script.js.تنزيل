function pageLoad() {
  $(".form-control").on("blur", function () {
    $(this).val(
      $(this)
        .val()
        .replace(/(<([^>]+)>)/gi, "")
    );
  });
  $(".showMoreBtn").on("click", function () {
    var allcomments = $(this)
      .parent()
      .children(" .comment-container.replies:hidden");
    $(allcomments).slice(0, 3).show();
    var allSelector = $(this).attr("targetDiv");
    if (
      $(
        '.all-comments[allDiv="' + allSelector + '"] .comment-container.replies'
      ).length == $(".all-comments .comment-container.replies:visible").length
    ) {
      $(".showMoreBtn").hide();
    }
  });
  ChangeArrow();
   $language = _spPageContextInfo.currentUICultureName;
   
  $(".portsSelect2").select2();
  $(".select2").select2({
 
    language: { noResults: () =>  $language == "ar-SA" ? "لاتوجد نتائج" : "No Results Found" },
    placeholder: "اختر",
  });
  $(".GridCustom div").addClass("table-responsive");
  $("#GridDiv div").addClass("table-responsive");
  $(
    "#ctl00_SPWebPartManager1_g_e14096a3_144b_4bf0_8cdf_0c0854a47bd7_ctl00_divAllSubLoyal div , #ctl00_SPWebPartManager1_g_e14096a3_144b_4bf0_8cdf_0c0854a47bd7_ctl00_divInfractionsSummary div"
  ).addClass("table-responsive");
  $(".DataPager").removeClass("table-responsive");

  if (
    $(
      ".datepicker-hijri:not([LocalInitiation]), .datepicker-gregorian:not([LocalInitiation])"
    ).length > 0
  )
    InitiateCalenders();

  var GridOne = $("div.GridOne").html();
  var GridTwo = $("div.GridTwo").html();
  $(".DiffGrid1").html(GridOne);
  $(".DiffGrid2").html(GridTwo);
  if (($("[id$='divRegImpOnly']").children().length = 2)) {
    $("[id$='divRegImpOnly']").find(".col-sm-1").addClass("col-sm-4");
  }
}

function getIsQuizPage() {
  context = new SP.ClientContext.get_current();
  web = context.get_web();
  listColl = web.get_lists();
  list = listColl.getById(_spPageContextInfo.pageListId);
  context.load(list);
  context.executeQueryAsync(
    Function.createDelegate(this, this.onQueryIsQuizzSucceeded),
    Function.createDelegate(this, this.onQueryIsQuizzFailed)
  );
}
function ClearAllControls() {
  for (i = 0; i < document.forms[0].length; i++) {
    doc = document.forms[0].elements[i];
    //console.log('doc.type');
    switch (doc.type) {
      case "text":
        doc.value = "";
        break;
      case "checkbox":
        doc.checked = false;
        break;
      case "radio":
        doc.checked = false;
        break;

      case "select-one":
        doc.selectedIndex = 0;
        break;
      case "select-multiple":
        doc.selectedIndex = 0;
        break;
      default:
        break;
    }
  }

  var curSelect2 = $(".groupingContent").find("select.select2");
  //console.log(curSelect2.length);
  for (var i = 0; i < curSelect2.length; i++) {
    //console.log(curSelect2[i]);
    $(curSelect2[i]).val(-1).trigger("change");
    $(curSelect2[i]).closest(".errorMsg").hide();
  }
}

function onQueryIsQuizzSucceeded() {
  if (list.get_baseTemplate() == "102") {
    jQuery("body").addClass("survey");
  }
}

function onQueryIsQuizzFailed(sender, args) {
  alert("Request failed. " + args.get_message() + "\n" + args.get_stackTrace());
}
jQuery;

function InitiateCalenders() {
  $language = _spPageContextInfo.currentUICultureName;
  var calendar = $.calendars.instance(
    "ummalqura",
    $language == "ar-SA" ? "ar" : ""
  );
  $(".datepicker-hijri").calendarsPicker({
    dateFormat: "yyyy/mm/dd",
    calendar: calendar,
    nextText: $language == "ar-SA" ? "التالي" : "Next",
    prevText: $language == "ar-SA" ? "السابق" : "Previous",
    todayText: $language == "ar-SA" ? "اليوم" : "Today",
    closeText: $language == "ar-SA" ? "اغلاق" : "Close",
    clearText: $language == "ar-SA" ? "مسح" : "Clear",
    yearRange: "c-40:c+5",
  });
  var calendarGregorian = $.calendars.instance(
    "gregorian",
    $language == "ar-SA" ? "ar-EG" : "en-US"
  );
  $(".datepicker-gregorian").calendarsPicker({
    dateFormat: "yyyy/mm/dd",
    calendar: calendarGregorian,
    nextText: $language == "ar-SA" ? "التالي" : "Next",
    prevText: $language == "ar-SA" ? "السابق" : "Previous",
    todayText: $language == "ar-SA" ? "اليوم" : "Today",
    closeText: $language == "ar-SA" ? "اغلاق" : "Close",
    clearText: $language == "ar-SA" ? "مسح" : "Clear",
    yearRange: "c-40:c+40",
  });
}

function ShowZatcaMessage(options) {
  if ($("#ZatcaMessageModal").length > 0) $("#ZatcaMessageModal").remove();
  $language = _spPageContextInfo.currentCultureName;
  var _modalDiv = $("<div>", {
    id: "ZatcaMessageModal",
    tabindex: "-1",
    class: "modal fade " + (options.modaldivcss ?? ""),
    role: "dialog",
    "aria-labelledby": "ZatcaMessageModalLabel",
    "aria-hidden": "true",
  });
  var _dialogueDiv = $("<div>", {
    role: "document",
    class: "modal-dialog modal-lg " + (options.dialoguedivcss ?? ""),
  });
  var _contentDiv = $("<div>", {
    class: "modal-content " + (options.contentdivcss ?? ""),
  });
  var _headerDiv = $("<div>", {
    class: "modal-header " + (options.headerdivcss ?? ""),
  });
  var _closeBtn = $("<button>", {
    type: "button",
    class: "close " + (options.closebtncss ?? ""),
    "data-dismiss": "modal",
  });
  $("<span>", { "aria-hidden": "true", class: options.closeiconcss ?? "" })
    .html("&times;")
    .appendTo(_closeBtn);
  _closeBtn.appendTo(_headerDiv);
  $("<h4>", { class: "modal-title fs-title " + (options.headercss ?? "") })
    .text(options.header)
    .appendTo(_headerDiv);
  _contentDiv.append(_headerDiv);

  var _bodyDiv = $("<div>", {
    class: "modal-body " + (options.bodydivcss ?? ""),
  });
  var _msgRowDiv = $("<div>", {
    class: "form-group " + (options.msgrowdivcss ?? ""),
  });
  var _msgDiv = $("<div>", {
    class: "col-sm-12 alert text-center " + (options.msgdivcss ?? ""),
  });
  if (options.msgtype == "confirm" || options.msgtype == "notify")
    _msgDiv.addClass("alert-info");
  else _msgDiv.addClass("alert-danger");
  var _msgLabel = $("<label></label>").text(options.message);
  _msgRowDiv.append(_msgDiv.append(_msgLabel));
  _bodyDiv.append(_msgRowDiv);
  _contentDiv.append(_bodyDiv);

  var _actionsClose = $("<button>", {
    type: "button",
    class:
      "btn btn-danger hvr-bounce-to-top " + (options.actionsclosecss ?? ""),
    "data-dismiss": "modal",
  }).text(
    options.actionclosetext ?? ($language == "ar-SA" ? "اغلاق" : "Close")
  );
  var _actionsReject = $("<button>", {
    type: "button",
    class:
      "btn btn-danger hvr-bounce-to-top " + (options.actionsrejectcss ?? ""),
    "data-dismiss": "modal",
  }).text(options.actionclosetext ?? ($language == "ar-SA" ? "رفض" : "Reject"));
  var _actionsConfirm = $("<button>", {
    type: "button",
    class:
      "btn btn-primary hvr-bounce-to-top " + (options.actionsconfirmcss ?? ""),
    "data-dismiss": "modal",
  }).text(
    options.actionconfirmtext ?? ($language == "ar-SA" ? "موافقة" : "Accept")
  );
  var _footerDiv = $("<div>", {
    class: "modal-footer " + (options.footerdivcss ?? ""),
  });

  if (options.msgtype == "confirm") {
    if (options.confirmcallback) _actionsConfirm.click(options.confirmcallback);
    if (options.rejectcallback) _actionsReject.click(options.rejectcallback);
    _footerDiv.append(_actionsConfirm).append(_actionsReject);
  } else {
    if (options.closecallback) _actionsClose.click(options.closecallback);
    _footerDiv.append(_actionsClose);
  }
  _contentDiv.append(_footerDiv);
  _dialogueDiv.append(_contentDiv);
  _modalDiv.append(_dialogueDiv);
  $("body").append(_modalDiv);
  $("#ZatcaMessageModal").modal("show");
  $("#ZatcaMessageModal").on("hidden.bs.modal", function () {
    $("#ZatcaMessageModal").remove();
    if (options.closecallback) options.closecallback();
  });
}

