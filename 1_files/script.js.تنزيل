  function gmod(n, m) {
    return ((n % m) + m) % m;
  }

  function kuwaiticalendar(adjust) {
    var today = new Date();
    if (adjust) {
      adjustmili = 1000 * 60 * 60 * 24 * adjust;
      todaymili = today.getTime() + adjustmili;
      today = new Date(todaymili);
    }
    day = today.getDate();
    month = today.getMonth();
    year = today.getFullYear();
    m = month + 1;
    y = year;
    if (m < 3) {
      y -= 1;
      m += 12;
    }
    a = Math.floor(y / 100);
    b = 2 - a + Math.floor(a / 4);
    if (y < 1583) b = 0;
    if (y == 1582) {
      if (m > 10) b = -10;
      if (m == 10) {
        b = 0;
        if (day > 4) b = -10;
      }
    }
    jd =
      Math.floor(365.25 * (y + 4716)) +
      Math.floor(30.6001 * (m + 1)) +
      day +
      b -
      1524;
    b = 0;
    if (jd > 2299160) {
      a = Math.floor((jd - 1867216.25) / 36524.25);
      b = 1 + a - Math.floor(a / 4);
    }
    bb = jd + b + 1524;
    cc = Math.floor((bb - 122.1) / 365.25);
    dd = Math.floor(365.25 * cc);
    ee = Math.floor((bb - dd) / 30.6001);
    day = bb - dd - Math.floor(30.6001 * ee);
    month = ee - 1;
    if (ee > 13) {
      cc += 1;
      month = ee - 13;
    }
    year = cc - 4716;
    wd = gmod(jd + 1, 7) + 1;
    iyear = 10631 / 30;
    epochastro = 1948084;
    epochcivil = 1948085;
    shift1 = 8.01 / 60;
    z = jd - epochastro;
    cyc = Math.floor(z / 10631);
    z = z - 10631 * cyc;
    j = Math.floor((z - shift1) / iyear);
    iy = 30 * cyc + j;
    z = z - Math.floor(j * iyear + shift1);
    im = Math.floor((z + 28.5001) / 29.5);
    if (im == 13) im = 12;
    id = z - Math.floor(29.5001 * im - 29);
    var myRes = new Array(8);
    myRes[0] = day;
    myRes[1] = month - 1;
    myRes[2] = year;
    myRes[3] = jd - 1;
    myRes[4] = wd - 1;
    myRes[5] = id; // adjust days from here
    myRes[6] = im - 1;
    myRes[7] = iy;
    return myRes;
  }

  function writeIslamicDate(adjustment) {
    if ($language == "ar-SA") {
      var wdNames = new Array(
        "الاحد",
        "الاثنين",
        "الثلاثاء",
        "الاربعاء",
        "الخميس",
        "الجمعه",
        "السبت"
      );
      var iMonthNames = new Array(
        "محرم",
        "صفر",
        "ربيع اول",
        "ربيع آخر",
        "جماد اول",
        "جماد آخر",
        "رجب",
        "شعبان",
        "رمضان",
        "شوال",
        "ذو القعده",
        "ذو الحجه"
      );
    } else {
      var wdNames = new Array(
        "Sunday",
        "Monday",
        "Tuesday",
        "Wednesday",
        "Thursday",
        "Friday",
        "Saturday"
      );
      var iMonthNames = new Array(
        "Moharam",
        "Safar",
        "Rabia Awal",
        "Rabia Akhar",
        "Gamad Awal",
        "Gamad Akhar",
        "Ragab",
        "Shaaban",
        "Ramadan",
        "Shawal",
        "Zu al keada",
        "Zu Al hegga"
      );
    }
    var iDate = kuwaiticalendar(adjustment);
    var outputIslamicDate =
      wdNames[iDate[4]] +
      " " +
      iDate[5] +
      " " +
      iMonthNames[iDate[6]] +
      " " +
      iDate[7];
    return outputIslamicDate;
  }
  var todayDate = new Date();
  var hour = todayDate.getHours();
  var min = todayDate.getMinutes();
  if ($language == "ar-SA") {
    if (hour > 11) {
      format = "م";
    } else {
      format = "ص";
    }
  } else {
    if (hour > 11) {
      format = "PM";
    } else {
      format = "AM";
    }
  }
  if (hour > 12) {
    hour = hour - 12;
  }
  if (hour == 0) {
    hour = 12;
  }
  if (min < 10) {
    min = "0" + min;
  }
  var d = new Date();
  var curr_date = d.getDate();
  var curr_month = d.getMonth();
  var curr_year = d.getFullYear();
  if ($language == "ar-SA") {
    var m_names = new Array(
        "يناير",
        "فبراير",
        "مارس",
        "ابريل",
        "مايو",
