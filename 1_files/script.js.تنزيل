

var localResources = {
  ar: {
   
    CopyRight:
      " هيئة الزكاة والضريبة والجمارك - جميع الحقوق محفوظة © " +
      new Date().getFullYear(),
    
  },
  en: {
  
    CopyRight:
      "Zakat, Tax and Customs Authority - all rights reserved © " +
      new Date().getFullYear(),
   
  },
};

var $util = function () {};

$util.currentLang =
  document.location.href.toLowerCase().indexOf("/en/") > 0 ? "en" : "ar";
$util.LangUrl =
  document.location.href.toLowerCase().indexOf("/en/") > 0 ? "/en" : "/ar";
$util.SwitchLangUrl =
  document.location.href.toLowerCase().indexOf("/en/") > 0 ? "/ar/" : "/en/";
$util.IsRTL =
  document.location.href.toLowerCase().indexOf("/en/") > 0 ? false : true;

$util.truncateString = function truncate(string, count) {
  if (string == undefined || string == null) return "";
  if (string.length > count)
    return (
      string.substring(0, string.substring(0, count).lastIndexOf(" ")) + ".."
    );
  else return string;
};

$util.setCookie = function (cname, cvalue, exdays) {
  var d = new Date();
  d.setDate(d.getDate() + exdays);
  var expires = "expires=" + d.toUTCString();
  document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
};

$util.getCookie = function (cname) {
  var name = cname + "=";
  var decodedCookie = decodeURIComponent(document.cookie);
  var ca = decodedCookie.split(";");
  for (var i = 0; i < ca.length; i++) {
    var c = ca[i];
    while (c.charAt(0) == " ") {
      c = c.substring(1);
    }
    if (c.indexOf(name) == 0) {
      return c.substring(name.length, c.length);
    }
  }
  return "";
};

// get the url from URL Fields which their value format is [URL], [Description]
$util.getUrlFromSPUrlField = function (url) {
  url = url ? $.trim(url.split(",")[0]) : url;
  return url;
};

$util.getSPCurrentSiteUrl = function () {
  var href = document.location.href.toLocaleLowerCase();
  return href
    .substring(href.indexOf("/" + $util.currentLang), href.lastIndexOf("/"))
    .replace("Pages", "")
    .replace("pages", "");
};

$util.getLocalString = function (key) {
  return localResources[$util.currentLang][key];
};

$util.getParameterByName = function (name) {
  name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
  var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
    results = regex.exec(location.search);
  return results === null
    ? ""
    : decodeURIComponent(results[1].replace(/\+/g, " "));
};


function ChangeArrow() {
  $("#GridDiv table tr th a").each(function (index, el) {
    $(this).addClass("GridAnchor");
    $(this).append(
      "<span class='fa fa-sort p5 text-muted'></span>"
    );
  });
}
if (!window.location.origin) {
  window.location.origin =
    window.location.protocol +
    "//" +
    window.location.hostname +
    (window.location.port ? ":" + window.location.port : "");
}

function GetURL(letter) {
  var letterDecoded = encodeURI(letter).toUpperCase();
  window.location.href =
    window.location.origin +
    window.location.pathname +
    "?Letter=" +
    letterDecoded;
}

function reset() {
  window.location.href = window.location.origin + window.location.pathname;
}

function Search() {
  var searchVal = document.getElementById("txtSearch").value;
  if (searchVal != "") {
    var searchDecoded = encodeURI(searchVal).toUpperCase();
    window.location.href =
      window.location.origin +
      window.location.pathname +
      "?Keyword=" +
      searchDecoded;
  }
}

function nextPrev(pageNumber) {
  var currentLoc = document.location.href,
    queryLoc = currentLoc.indexOf("p="),
    queryExist = currentLoc.indexOf("?");
  if (queryLoc != -1) {
    currentLoc = currentLoc.substr(0, queryLoc) + "p=" + pageNumber;
  } else if (queryExist == -1) {
    currentLoc = currentLoc + "?p=" + pageNumber;
  } else {
    currentLoc = currentLoc + "&p=" + pageNumber;
  }
  document.location = currentLoc;
}

function pagingControl() {
  var paging = $(".input-field input").val(),
    currentLoc = document.location.href,
    queryLoc = currentLoc.indexOf("p="),
    queryExist = currentLoc.indexOf("?");
  if (paging >= 1 && paging <= $(".input-field input").attr("max")) {
    if (queryLoc != -1) {
      currentLoc = currentLoc.substr(0, queryLoc) + "p=" + paging;
    } else if (queryExist == -1) {
      currentLoc = currentLoc + "?p=" + paging;
    } else {
      currentLoc = currentLoc + "&p=" + paging;
    }
    document.location = currentLoc;
  } else {
    alert("please enter a valid number");
    return false;
  }
}
$(".paging .input-field input").keypress(function (e) {
  if (e.which == 13) {
    pagingControl();
  }
});

function addQuery(videoName) {
  event.preventDefault();
  var currentLoc = document.location.href,
    queryLoc = currentLoc.indexOf("&"),
    queryExist = currentLoc.indexOf("?type");
  if (queryLoc != -1) {
    currentLoc = currentLoc.substr(0, queryLoc) + "&VName=" + videoName;
  } else {
    currentLoc = currentLoc + "&VName=" + videoName;
  }
  document.location = currentLoc;
}

function videoPage(pageName, videoPage) {
  var queryLoc = pageName.indexOf("LandingPage.aspx");
  if (queryLoc != -1) {
    currentLoc =
      pageName.substr(0, queryLoc) +
      "VideoPage.aspx?SCFLD=" +
      videoPage +
      "&VName=" +
      videoPage;
  } else {
    if ($language == "ar-SA") {
      currentLoc =
        window.location.protocol +
        "//" +
        window.location.host +
        _spPageContextInfo.siteServerRelativeUrl +
        "/ar" +
        "/Media/Videos/Pages/Pages/VideoPage.aspx?SCFLD=" +
        videoPage +
        "&VName=" +
        videoPage;
    } else {
      currentLoc =
        window.location.protocol +
        "//" +
        window.location.host +
        _spPageContextInfo.siteServerRelativeUrl +
        "/en" +
        "/Media/Videos/Pages/Pages/VideoPage.aspx?SCFLD=" +
        videoPage +
        "&VName=" +
        videoPage;
    }
  }
  document.location = currentLoc;
}

function imagesPage(pageName, imagePage) {
  var queryLoc = pageName.indexOf("LandingPage.aspx");
  if (queryLoc != -1) {
    currentLoc =
      pageName.substr(0, queryLoc) + "ImagesAlbumPage.aspx?SCFLD=" + imagePage;
  } else {
    if ($language == "ar-SA") {
      currentLoc =
        window.location.protocol +
        "//" +
        window.location.host +
        _spPageContextInfo.siteServerRelativeUrl +
        "/ar" +
        "/Media/PIcLib/Pages/Pages/ImagesAlbumPage.aspx?SCFLD=" +
        imagePage;
    } else {
      currentLoc =
        window.location.protocol +
        "//" +
        window.location.host +
        _spPageContextInfo.siteServerRelativeUrl +
        "/en" +
        "/Media/PIcLib/Pages/Pages/ImagesAlbumPage.aspx?SCFLD=" +
        imagePage;
    }
  }
  document.location = currentLoc;
}
$(document).on("click", ".language-select", function (e) {
  var currentLang;
  var otherLang;
  var websiteaddr = window.location.protocol + "://" + window.location.hostname;
  if (
    window.location.href.indexOf("/en/") > -1 ||
    window.location.href.endsWith("/en")
  ) {
    currentLang = "/en";
    otherLang = "/ar";
  } else {
    currentLang = "/ar";
    otherLang = "/en";
  }
  var url = window.location.href;
  if (url == "/") {
    url = "/ar";
  }
  window.location.href = url.replace(currentLang, otherLang);
});
