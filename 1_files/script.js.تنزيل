function CheckAge(source, arguments) {
  if (arguments == undefined) {
    arguments.IsValid = true;
    return;
  }

  var isOnAge = false;
  var date1 = $(source).parent().find('[id$="txtStartDate"]').val();
  var date2 = $("[id*= hdnCurrentDate]").val();

  var date1D = new Date(date1);
  var date2D = new Date(date2);

  var diff = date2D - date1D.setFullYear(date1D.getFullYear() + parseInt(18));

  if (diff >= 0) {
    arguments.IsValid = true;
  } else {
    arguments.IsValid = false;
  }
}

function checkDateNotAfterToday(source, arguments) {
  var newDate1 = Date.parse(
    $(source).parent().find('[id$="txtStartDate"]').val()
  );
  var newDate2 = Date.parse($("[id*= hdnCurrentDate]").val());
  if (newDate1 > newDate2) {
    arguments.IsValid = false;
  } else {
    arguments.IsValid = true;
  }
}
function checkDateNotBeforeToday(source, arguments) {
  var newDate1 = Date.parse(
    $(source).parent().find('[id$="txtStartDate"]').val()
  );
  var newDate2 = Date.parse($("[id*= hdnCurrentDate]").val());
  if (newDate1 < newDate2) {
    arguments.IsValid = false;
  } else {
    arguments.IsValid = true;
  }
}
